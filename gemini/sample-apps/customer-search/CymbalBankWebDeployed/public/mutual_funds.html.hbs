                console.log(references)
                var str = '';
                var i = 0;
                var count = 0


            })
            .catch(err => {
                console.log('Fetch Error :-S', err);
            })
            .finally(() => {
                // Hide loading symbol
                document.getElementById('loader').style.display = 'none';
            });;


    }


    function searchKeyPress(e) {

